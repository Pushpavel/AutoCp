CREATE TABLE Testcase (
    name TEXT NOT NULL,
    input TEXT NOT NULL,
    output TEXT NOT NULL,
    problemName TEXT NOT NULL,
    problemGroup TEXT NOT NULL,
    FOREIGN KEY(problemName,problemGroup) REFERENCES ProblemInfo(name,problemGroup),
    PRIMARY KEY(name,problemName,problemGroup)
);

insertTestcase:
    INSERT INTO Testcase
    VALUES ?;

getTestcases:
    SELECT * FROM Testcase
    WHERE problemName = ? AND problemGroup = ?;